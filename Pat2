
public class Pat2 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		
		
		int N =10;
		
		
		for(int i = 1; i <=N;i++)
		{
			for(int k = N; k>= i;k--)
				System.out.print(" ");
			
			for(int j = 1; j<=i;j++)
			{
				
				
				
				System.out.print("*");
				
			}
			System.out.println();
			
		}

	}

}
