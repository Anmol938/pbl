import java.math.*;
public class BigAdd
{

public static voidmain(String args[])
{

String input1 = "11111111111111111111111111111";
String input2 = "22222222222222222222222222222";
	BigInteger b1,b2;


			b1 = new BigInteger( input1);
			b2 = new BigInteger( input2);

	return b1.add(b2).toString();



	}
}
