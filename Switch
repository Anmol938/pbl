
public class Switch {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		
		int month = 123;
		
		
		switch(month)
		{
		
		case 1:
			System.out.println("Jan");
			break;
		case 2:
				System.out.println("Feb");
				break;
		case 3:
				System.out.println("Mar");
				break;
		case 4:
				System.out.println("Apr");
				break;
		default:
				System.out.println("JAAT_DEVTA");
				break;
				
		
		}
		
		
		
		
		
		
	}

}
