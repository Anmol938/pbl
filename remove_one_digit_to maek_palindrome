String rev = Integer.toString(input1);
				StringBuilder ob = new StringBuilder(rev);

		String rev1 = ob.reverse().toString();
				if( input1 == Integer.parseInt(rev1))
						return -1;
				else
				{
						int count[] = new int[10];

					while(input1 > 0 )
					{
						count[input1%10]++;
						input1/=10;
					}

					int index = -1;

	for(int i = 0 ; i < count.length;i++)
	{
			if(  count[i] == 1 )
					index = i;


	}

return index;


				}

