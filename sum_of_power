package Aloha;
import java.math.*;
public class Po {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		
		int num = 582109;
		
		
		String str = Integer.toString(num);
		
		int sum = 0 ;
		for(int i = 0 ; i < str.length()-1;i++ )
		{
			
				sum +=   Math.pow(    Character.getNumericValue( str.charAt(i) ), Character.getNumericValue(str.charAt(i+1)) ) ;
				
			
			
		}
		
		sum += 			Math.pow(	Character.getNumericValue( str.charAt(str.length()-1)) , 0);
		
		System.out.println(sum);
		
		
	}

}
